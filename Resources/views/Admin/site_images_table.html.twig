<table class="table table-striped table-bordered table-hover table-checkable table-responsive datatable dataTable" data-display-length="25" id="site-images-table" aria-describedby="site-images-table_info">
    <thead>
    <tr role="row">
        <th class="checkbox-column sorting_disabled" role="columnheader" rowspan="1" colspan="1" aria-label="">
            <div class="checker">
                <span>
                    <input type="checkbox" class="uniform" data-master-checkbox>
                </span>
            </div>
        </th>
        <th data-class="expand" class="sorting" role="columnheader" tabindex="0" aria-controls="site-images-table" rowspan="1" colspan="1" aria-label="Image" width="80">
            Image
        </th>
        <th data-class="expand" class="sorting" role="columnheader" tabindex="0" aria-controls="site-images-table" rowspan="1" colspan="1" aria-label="Title">
            Title
        </th>
        <th data-hide="phone" class="sorting" role="columnheader" tabindex="0" aria-controls="site-images-table" rowspan="1" colspan="1" aria-label="Width" width="80">
            Width
        </th>
        <th data-hide="phone,tablet" class="sorting" role="columnheader" tabindex="0" aria-controls="site-images-table" rowspan="1" colspan="1" aria-label="Height" width="80">
            Height
        </th>
        <th data-hide="phone,tablet" class="sorting" role="columnheader" tabindex="0" aria-controls="site-images-table" rowspan="1" colspan="1" aria-label="Options" width="125">
            Actions
        </th>
    </tr>
    </thead>

    <tbody role="alert" aria-live="polite" aria-relevant="all">
        {% for i,image in images %}
            {% include 'CoreSysSiteBundle:Admin:site_images_table_row.html.twig' %}
        {% endfor %}
    </tbody>
</table>